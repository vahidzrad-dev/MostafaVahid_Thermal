	Journal = {Biophys. J.},
	Keywords = {RED-BLOOD-CELLS; SHAPE TRANSFORMATIONS; PHOSPHOLIPID-VESICLES; BILAYER; VESICLES; MINIMUM ENERGY; LIPID BILAYERS; MEMBRANES; CLATHRIN; CHOLESTEROL; CURVATURE; Biophysics},
	Language = {English},
